{
  manifest_version: "0.1.0",
  program: {
    image: "ghcr.io/car-bench/car-bench-agentbeats-car-bench-agent:latest",
    entrypoint: [
      "uv", 
      "run", 
      "src/purple_car_bench_agent/server.py", 
      "--host", 
      "0.0.0.0", 
      "--port", 
      "8080"
    ],
    env: {
      AGENT_LLM: "${config.agent_llm}",
      PROXY_URL: "${slots.proxy.url}",
      ANTHROPIC_API_KEY: "${config.anthropic_api_key}",
    },
    network: {
      endpoints: [
        { name: "endpoint", port: 8080 },
      ],
    },
  },
   config_schema: {
    type: "object",
    properties: {
      agent_llm: { type: "string" },
      anthropic_api_key: { type: "string", secret: true },
    },
    required: ["agent_llm", "anthropic_api_key"],
    additionalProperties: false,
  },
  slots: {
    proxy: { kind: "a2a" },
  },
  provides: {
    a2a: { kind: "a2a", endpoint: "endpoint" },
  },
  exports: {
    a2a: "a2a",
  },
}